  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT f.FilmName, f.FilmReleaseDate, f.FilmRunTimeMinutes, d.DirectorName, c.CountryName
FROM tblFilm f
JOIN tblDirector d
ON f.FilmDirectorID = d.DirectorID
JOIN tblCountry c
ON f.FilmCountryID = c.CountryID</CommandText>
      </Query>
