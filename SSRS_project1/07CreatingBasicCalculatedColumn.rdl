  <DataSets>
    <DataSet Name="dtsFinance">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT f.FilmName, f.FilmBudgetDollars, f.FilmBoxOfficeDollars, f.FilmRunTimeMinutes, d.DirectorName, f.FilmReleaseDate
FROM tblFilm f
Join tblDirector d
ON f.FilmDirectorID = d.DirectorID</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmBudgetDollars">
          <DataField>FilmBudgetDollars</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FilmBoxOfficeDollars">
          <DataField>FilmBoxOfficeDollars</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FilmPL">
          <Value>= Fields!FilmBoxOfficeDollars.Value-Fields!FilmBudgetDollars.Value</Value>
        </Field>
        <Field Name="FilmRunTimeMinutes">
          <DataField>FilmRunTimeMinutes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FilmPLDescription">
          <Value>=IIf(Fields!FilmBoxOfficeDollars.Value&gt;Fields!FilmBudgetDollars.Value, "Profit", "Loss")</Value>
        </Field>
        <Field Name="FilmReleaseDate">
          <DataField>FilmReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <Value>=Switch(Fields!FilmRunTimeMinutes.Value &lt; 90, "short", Fields!FilmRunTimeMinutes.Value &lt; 150, "Medium", True, "Long")</Value>
        </Field>
        <Field Name="dd">
          <Value>=Fields!FilmName.Value &amp; " is a " &amp; Fields!Description.Value &amp; " Movie."</Value>
        </Field>
        <Field Name="FirstName">
          <Value>=Left(Fields!DirectorName.Value,InStr(Fields!DirectorName.Value, " ")-1)</Value>
        </Field>
        <Field Name="LastName">
          <Value>=Right(Fields!DirectorName.Value, Len(Fields!DirectorName.Value) - InStr(Fields!DirectorName.Value, " "))</Value>
        </Field>
        <Field Name="Month">
          <Value>=Month(Fields!FilmReleaseDate.Value)</Value>
        </Field>
        <Field Name="FilmMonthName">
          <Value>=MonthName(Fields!Month.Value)</Value>
        </Field>
        <Field Name="FilmAge">
          <Value>=DateDiff(DateInterval.Year, Fields!FilmReleaseDate.Value, Today())</Value>
        </Field>
        <Field Name="FilmBirthday">
          <Value>=DateAdd(dateinterval.Year, Fields!FilmAge.Value,Fields!FilmReleaseDate.Value)</Value>
        </Field>
        <Field Name="FilmAgeAccurate">
          <Value>=IIf(Fields!FilmBirthday.Value&gt;Today(), Fields!FilmAge.Value-1, Fields!FilmAge.Value)</Value>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
